{
    "name": "@moso/eslint-config",
    "type": "module",
    "version": "1.0.2",
    "packageManager": "bun@1.2.5",
    "description": "moso's shared ESLint config",
    "author": {
        "name": "Morten SÃ¸rensen",
        "email": "morten@moso.dev",
        "url": "https://moso.dev"
    },
    "license": "MIT",
    "homepage": "https://github.com/moso/eslint-config",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moso/eslint-config/eslint-config.git"
    },
    "bugs": {
        "url": "https://github.com/moso/eslint-config/issues"
    },
    "keywords": [
        "eslint-config",
        "eslint",
        "config",
        "eslint-config moso",
        "moso",
        "typescript",
        "react",
        "vue"
    ],
    "exports": {
        ".": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": ["dist"],
    "scripts": {
        "build": "bun run typegen && tsup --clean --dts",
        "build:inspector": "bun run build && bunx @eslint/config-inspector build",
        "dev": "bunx @eslint/config-inspector --config eslint.config.ts",
        "lint": "eslint",
        "prepack": "bun run build",
        "prepare": "bun run simple-git-hooks",
        "release": "bumpp && bun publish",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "typegen": "tsx src/typegen/typegen.ts",
        "watch": "tsup --watch"
    },
    "peerDependencies": {
        "eslint": "^9.10.0"
    },
    "dependencies": {
        "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
        "@eslint-react/eslint-plugin": "^1.34.1",
        "@stylistic/eslint-plugin": "^4.2.0",
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "@vitest/eslint-plugin": "^1.1.37",
        "eslint-config-flat-gitignore": "^2.1.0",
        "eslint-flat-config-utils": "^2.0.1",
        "eslint-merge-processors": "^2.0.0",
        "eslint-plugin-antfu": "^3.1.1",
        "eslint-plugin-import-x": "^4.8.0",
        "eslint-plugin-jsdoc": "^50.6.6",
        "eslint-plugin-jsonc": "^2.19.1",
        "eslint-plugin-n": "^17.16.2",
        "eslint-plugin-no-only-tests": "^3.3.0",
        "eslint-plugin-perfectionist": "^4.10.1",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "eslint-plugin-unicorn": "^57.0.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "eslint-plugin-vue": "^10.0.0",
        "eslint-plugin-yml": "^1.17.0",
        "eslint-processor-vue-blocks": "^2.0.0",
        "globals": "^16.0.0",
        "jsonc-eslint-parser": "^2.4.0",
        "local-pkg": "^1.1.1",
        "vue-eslint-parser": "^10.1.1",
        "yaml-eslint-parser": "^1.3.0"
    },
    "devDependencies": {
        "@eslint-types/jsdoc": "^48.2.2",
        "@eslint-types/typescript-eslint": "^7.5.0",
        "@eslint-types/unicorn": "^52.0.0",
        "@eslint/config-inspector": "^1.0.2",
        "@types/eslint": "^9.6.1",
        "@types/node": "^22.13.10",
        "bumpp": "^10.1.0",
        "eslint": "^9.22.0",
        "eslint-typegen": "^2.1.0",
        "jiti": "^2.4.2",
        "lint-staged": "^15.5.0",
        "simple-git-hooks": "^2.11.1",
        "tsup": "^8.4.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "vitest": "^3.0.8",
        "vue": "^3.5.13"
    },
    "resolutions": {
        "@eslint-community/eslint-utils": "^4.4.1",
        "@typescript-eslint/utils": "^8.26.1",
        "eslint": "^9.22.0",
        "tsx": "^4.19.3"
    },
    "simple-git-hooks": {
        "pre-commit": "bunx lint-staged"
    },
    "lint-staged": {
        "*": "eslint --fix"
    }
}
