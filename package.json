{
    "name": "@moso/eslint-config",
    "type": "module",
    "version": "2.0.5",
    "description": "My shareable ESLint config",
    "author": {
        "name": "Morten SÃ¸rensen",
        "email": "morten@moso.dev",
        "url": "https://moso.dev"
    },
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/moso/eslint-config",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moso/eslint-config/eslint-config.git"
    },
    "bugs": {
        "url": "https://github.com/moso/eslint-config/issues"
    },
    "keywords": [
        "config",
        "eslint",
        "eslint-config",
        "eslint-config moso",
        "functional programming",
        "moso",
        "perfectionist",
        "stylistic",
        "typescript",
        "react",
        "vue"
    ],
    "exports": {
        ".": "./dist/index.mjs",
        "./package.json": "./package.json"
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "files": [
        "LICENSE",
        "README.md",
        "dist/",
        "package.json"
    ],
    "scripts": {
        "build": "bun run typegen && tsdown --clean --dts",
        "build:inspector": "bun run build && bunx @eslint/config-inspector build",
        "dev": "bunx @eslint/config-inspector --config eslint.config.ts",
        "lint": "eslint",
        "prepack": "bun run build",
        "prepare": "bun run simple-git-hooks",
        "release": "bumpp && bun publish",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "typegen": "tsx scripts/typegen.ts",
        "watch": "tsdown --watch"
    },
    "workspaces": ["src/rules"],
    "peerDependencies": {
        "@eslint-react/eslint-plugin": "^2.4.0",
        "@next/eslint-plugin-next": "^16.1.1",
        "astro-eslint-parser": "^1.2.2",
        "eslint": ">=9.30.0",
        "eslint-plugin-astro": "^1.5.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.1",
        "eslint-plugin-vuejs-accessibility": "^2.4.1"
    },
    "peerDependenciesMeta": {
        "@eslint-react/eslint-plugin": {
            "optional": true
        },
        "@next/eslint-plugin-next": {
            "optional": true
        },
        "astro-eslint-parser": {
            "optional": true
        },
        "eslint-plugin-astro": {
            "optional": true
        },
        "eslint-plugin-jsx-a11y": {
            "optional": true
        },
        "eslint-plugin-react-hooks": {
            "optional": true
        },
        "eslint-plugin-react-refresh": {
            "optional": true
        },
        "eslint-plugin-react-you-might-not-need-an-effect": {
            "optional": true
        },
        "eslint-plugin-vuejs-accessibility": {
            "optional": true
        }
    },
    "dependencies": {
        "@antfu/install-pkg": "^1.1.0",
        "@clack/prompts": "^0.11.0",
        "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
        "@stylistic/eslint-plugin": "^5.6.1",
        "@typescript-eslint/eslint-plugin": "^8.50.1",
        "@typescript-eslint/parser": "^8.50.1",
        "@vitest/eslint-plugin": "^1.6.4",
        "eslint-flat-config-utils": "^2.1.4",
        "eslint-merge-processors": "^2.0.0",
        "eslint-plugin-de-morgan": "^2.0.0",
        "eslint-plugin-functional": "^9.0.2",
        "eslint-plugin-import-lite": "^0.4.0",
        "eslint-plugin-jsdoc": "^61.5.0",
        "eslint-plugin-jsonc": "^2.21.0",
        "eslint-plugin-n": "^17.23.1",
        "eslint-plugin-no-only-tests": "^3.3.0",
        "eslint-plugin-perfectionist": "^5.1.0",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-regexp": "^2.10.0",
        "eslint-plugin-unicorn": "^62.0.0",
        "eslint-plugin-unused-imports": "^4.3.0",
        "eslint-plugin-yml": "^1.19.1",
        "eslint-processor-vue-blocks": "^2.0.0",
        "globals": "^16.5.0",
        "jsonc-eslint-parser": "^2.4.2",
        "local-pkg": "^1.1.2",
        "toml-eslint-parser": "^0.10.1",
        "vue-eslint-parser": "^10.2.0",
        "yaml-eslint-parser": "^1.3.2"
    },
    "devDependencies": {
        "@eslint-react/eslint-plugin": "^2.4.0",
        "@eslint/compat": "^2.0.0",
        "@eslint/config-inspector": "^1.4.2",
        "@eslint/js": "^9.39.2",
        "@moso/eslint-plugin": "workspace:*",
        "@next/eslint-plugin-next": "^16.1.1",
        "@types/node": "^25.0.3",
        "@typescript-eslint/rule-tester": "^8.50.1",
        "@typescript-eslint/utils": "^8.50.1",
        "astro-eslint-parser": "^1.2.2",
        "bumpp": "^10.3.2",
        "eslint": "^9.39.2",
        "eslint-plugin-astro": "^1.5.0",
        "eslint-plugin-erasable-syntax-only": "^0.4.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.1",
        "eslint-plugin-toml": "^0.12.0",
        "eslint-plugin-vue": "^10.6.2",
        "eslint-plugin-vuejs-accessibility": "^2.4.1",
        "eslint-typegen": "^2.3.0",
        "jiti": "^2.6.1",
        "lint-staged": "^16.2.7",
        "simple-git-hooks": "git+https://github.com/toplenboren/simple-git-hooks.git",
        "ts-dedent": "^2.2.0",
        "tsdown": "^0.18.3",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "vitest": "^4.0.16"
    },
    "simple-git-hooks": {
        "pre-commit": "bunx lint-staged"
    },
    "lint-staged": {
        "*": "eslint --fix"
    },
    "trustedDependencies": [
        "esbuild",
        "simple-git-hooks",
        "unrs-resolver"
    ]
}
