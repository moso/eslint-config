{
    "name": "@moso/eslint-config",
    "version": "1.0.0",
    "description": "moso's shared ESLint config",
    "type": "module",
    "keywords": [
        "eslint-config",
        "eslint",
        "config",
        "eslint-config moso",
        "moso",
        "typescript",
        "react",
        "vue"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --clean --dts",
        "dev": "tsup src/index.ts --format esm,cjs --watch && npx @eslint/config-inspector",
        "lint": "bun run stub && eslint .",
        "lint:fix": "bun run stub && eslint . --fix",
        "release": "bumpp && npm publish",
        "stub": "tsup src/index.ts --format esm",
        "test": "vitest",
        "typecheck": "tsc --noEmit"
    },
    "peerDependencies": {
        "eslint": ">=8.53.0 || 9.0.0"
    },
    "dependencies": {
        "@antfu/eslint-define-config": "latest",
        "@arthurgeron/eslint-plugin-react-usememo": "^2.3.1",
        "@eslint-types/jsdoc": "48.2.2",
        "@eslint-types/typescript-eslint": "^7.5.0",
        "@eslint-types/unicorn": "^52.0.0",
        "@stylistic/eslint-plugin": "^1.7.0",
        "@typescript-eslint/eslint-plugin": "7.6.0",
        "@typescript-eslint/parser": "^7.6.0",
        "eslint-config-flat-gitignore": "^0.1.5",
        "eslint-merge-processors": "^0.1.0",
        "eslint-plugin-antfu": "^2.1.2",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import-x": "^0.5.0",
        "eslint-plugin-jsdoc": "^48.2.3",
        "eslint-plugin-jsonc": "^2.15.1",
        "eslint-plugin-markdown": "^4.0.1",
        "eslint-plugin-n": "^17.2.1",
        "eslint-plugin-no-only-tests": "^3.1.0",
        "eslint-plugin-perfectionist": "^2.9.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "eslint-plugin-unicorn": "^52.0.0",
        "eslint-plugin-unused-imports": "^3.1.0",
        "eslint-plugin-vitest": "^0.5.3",
        "eslint-plugin-vue": "^9.25.0",
        "eslint-plugin-yml": "^1.14.0",
        "eslint-processor-vue-blocks": "^0.1.1",
        "globals": "^15.0.0",
        "jsonc-eslint-parser": "^2.4.0",
        "local-pkg": "^0.5.0",
        "vue-eslint-parser": "^9.4.2",
        "yaml-eslint-parser": "^1.2.2"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^0.4.6",
        "@stylistic/eslint-plugin-migrate": "^1.7.0",
        "@types/eslint": "^8.56.7",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.12.6",
        "@typescript-eslint/rule-tester": "^7.6.0",
        "bumpp": "^9.4.0",
        "eslint": "^9.0.0",
        "eslint-flat-config-viewer": "^0.1.20",
        "eslint-flat-config-utils": "^0.2.1",
        "esno": "^4.7.0",
        "fast-glob": "^3.3.2",
        "fs-extra": "^11.2.0",
        "lint-staged": "^15.2.2",
        "rimraf": "^5.0.5",
        "simple-git-hooks": "^2.11.1",
        "tsup": "^8.0.2",
        "typescript": "^5.4.4",
        "vitest": "^1.4.0"
    },
    "author": {
        "name": "Morten SÃ¸rensen",
        "email": "morten@moso.io",
        "url": "https://moso.io"
    },
    "license": "MIT",
    "homepage": "https://moso.io",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moso/eslint-config/eslint-config.git"
    },
    "bugs": {
        "url": "https://github.com/moso/eslint-config/issues"
    }
}
