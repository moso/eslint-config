{
    "name": "@moso/eslint-config",
    "version": "1.0.0",
    "description": "moso's shared ESLint config",
    "type": "module",
    "keywords": [
        "eslint-config",
        "eslint",
        "config",
        "eslint-config moso",
        "moso",
        "typescript",
        "react",
        "vue"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf ./dist && tsup src/index.ts --format esm,cjs --clean --dts",
        "dev": "rimraf ./dist && tsup src/index.ts --format esm,cjs --watch && npx @eslint/config-inspector",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "release": "bumpp && npm publish",
        "typecheck": "tsc --noEmit"
    },
    "peerDependencies": {
        "eslint": ">=8.53.0 || 9"
    },
    "dependencies": {
        "@antfu/eslint-define-config": "latest",
        "@arthurgeron/eslint-plugin-react-usememo": "^2.4.1",
        "@eslint-types/jsdoc": "48.2.2",
        "@eslint-types/typescript-eslint": "^7.5.0",
        "@eslint-types/unicorn": "^52.0.0",
        "@stylistic/eslint-plugin": "^2.7.2",
        "@typescript-eslint/eslint-plugin": "8.4.0",
        "@typescript-eslint/parser": "^8.4.0",
        "eslint-config-flat-gitignore": "^0.3.0",
        "eslint-merge-processors": "^0.1.0",
        "eslint-plugin-antfu": "^2.5.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import-x": "^4.2.1",
        "eslint-plugin-jsdoc": "^50.2.2",
        "eslint-plugin-jsonc": "^2.16.0",
        "eslint-plugin-markdown": "^5.1.0",
        "eslint-plugin-n": "^17.10.2",
        "eslint-plugin-no-only-tests": "^3.3.0",
        "eslint-plugin-perfectionist": "^3.5.0",
        "eslint-plugin-react": "^7.35.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.11",
        "eslint-plugin-unicorn": "^55.0.0",
        "eslint-plugin-unused-imports": "^4.1.3",
        "eslint-plugin-vitest": "^0.5.4",
        "eslint-plugin-vue": "^9.28.0",
        "eslint-plugin-yml": "^1.14.0",
        "eslint-processor-vue-blocks": "^0.1.2",
        "globals": "^15.9.0",
        "jsonc-eslint-parser": "^2.4.0",
        "local-pkg": "^0.5.0",
        "vue-eslint-parser": "^9.4.3",
        "yaml-eslint-parser": "^1.2.3"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^0.5.4",
        "@types/eslint": "^9.6.1",
        "@types/node": "^22.5.4",
        "bumpp": "^9.5.2",
        "eslint": "^9.10.0",
        "eslint-flat-config-utils": "^0.4.0",
        "fast-glob": "^3.3.2",
        "rimraf": "^6.0.1",
        "simple-git-hooks": "^2.11.1",
        "tsup": "^8.2.4",
        "typescript": "^5.5.4",
        "vitest": "^2.0.5"
    },
    "author": {
        "name": "Morten SÃ¸rensen",
        "email": "morten@moso.io",
        "url": "https://moso.io"
    },
    "license": "MIT",
    "homepage": "https://moso.io",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moso/eslint-config/eslint-config.git"
    },
    "bugs": {
        "url": "https://github.com/moso/eslint-config/issues"
    }
}
